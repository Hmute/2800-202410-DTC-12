<%- include('templates/header') %>
<h1>Set Your Weight Goal</h1>
<form action="/health/setWeightGoal" method="post">
  <div>
    <label for="weightGoal">Weight Goal (kg):</label>
    <input type="number" id="weightGoal" name="weightGoal" required>
  </div>
  <div>
    <label for="goalTimeframe">Goal Timeframe (weeks):</label>
    <input type="number" id="goalTimeframe" name="goalTimeframe" required>
  </div>
  <button type="submit">Submit</button>
</form>
<%- include('templates/footer') %>


